<?xml version="1.0" encoding="utf-8"?>
<CaseMapTable>
    <!--所有尺寸单位以mm计算-->
    <!--定义纸张尺寸-->
    <Paper size="A4" width="842" height="1000">
        <!--定义纸张边距，上，左，右，下-->
        <Margin top="0"  left="0" right="0" bottom="3" />
        <orientation isPortrait="No" />
    </Paper>
    <!--所有缺省设置元素，default*之类均必不可少-->
    <!--固定表格-->
    <StaticTable>
        <Lines>
            <!--默认线宽-->
            <defaultLineWidth>0.4</defaultLineWidth>
            <!--上线-->
            <line x1="19" y1="26" x2="279" y2="26" lineWidth="0.6"/>
            <!--下线-->
            <line x1="19" y1="350" x2="279" y2="350" lineWidth="0.6"/>
            <!-- 左边框 -->
            <line x1="19" y1="26" x2="19" y2="350" lineWidth="0.6"/>
            <!--右边框-->
            <line x1="279" y1="26" x2="279" y2="350" lineWidth="0.6"/>
            
            <!--简介 下线-->
             <line x1="19" y1="36" x2="279" y2="36" lineWidth="0.6"/>
            <!--列1 下线-->
            <line x1="19" y1="46" x2="279" y2="46" lineWidth="0.6"/>
            <!--列2 下线-->
            <line x1="19" y1="56" x2="279" y2="56" lineWidth="0.6"/>
            
            
            <!--简介 序号 右列 分割线-->
            <line x1="35" y1="26" x2="35" y2="56" lineWidth="0.6"/>
            <!--简介 检查时间 右列 分割线-->
            <line x1="129" y1="26" x2="129" y2="56" lineWidth="0.6"/>
            
            
            <!--简介 责任区起止桩号 右列 分割线-->
            <line x1="214" y1="26" x2="214" y2="56" lineWidth="0.6"/>
            
            
            <!--内容 第一个行中间分割线-->
             <line x1="19" y1="154" x2="279" y2="154" lineWidth="0.6"/>
            <!--内容 第二个行中间分割线-->
            <line x1="19" y1="252" x2="279" y2="252" lineWidth="0.6"/>
            
            <!--第一个内容框 图片跟文字的分割虚线-->
            <line x1="19" y1="140" x2="279" y2="140" lineWidth="0.6"/>
            
            
            <!--第二个内容框 图片跟文字的分割虚线-->
            <line x1="19" y1="238" x2="279" y2="238" lineWidth="0.6"/>
            
            <!--第三个内容框 图片跟文字的分割虚线-->
            <line x1="19" y1="336" x2="279" y2="336" lineWidth="0.6"/>
            
            
            <!--列正中间分割线-->
            <line x1="149" y1="26" x2="149" y2="350" lineWidth="0.6"/>

            
        </Lines>
        <Texts>
            <defaultFontSize>19</defaultFontSize>
            <!--标题:-->
            <Text>
                <text>构 造 物 检 查 记 录 表</text>
                <fontSize>23</fontSize>
                <origin x="100" y="10" />
                <size width="500" height="6" />
            </Text>
            <Text>
                <text>巡查日期：</text>
                <origin x="120" y="20" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>年</text>
                <origin x="148" y="20" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>月</text>
                <origin x="162" y="20" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>日</text>
                <origin x="173" y="20" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>序号</text>
                <origin x="23" y="32" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>1</text>
                <origin x="25" y="42" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>2</text>
                <origin x="25" y="52" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>检查时间</text>
                <origin x="70" y="32" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>至</text>
                <origin x="80" y="42" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>天气</text>
                <origin x="135" y="32" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>责任区起止桩号</text>
                <origin x="170" y="32" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>至</text>
                <origin x="180" y="42" />
                <size width="100" height="6" />
            </Text>
            <Text>
                <text>检查人员</text>
                <origin x="240" y="32" />
                <size width="100" height="6" />
            </Text>
        </Texts>
    </StaticTable>
    <DataTable>
        <!--设置默认字体-->
        <DefaultFontSize>19</DefaultFontSize>
        <!--时间-->
        <!--年-->
        <UITextField>
            <origin x="138" y="18" />
            <size width="18" height = "6"/>
            <content>
                <data>
                    <attributeName>inspectiondate</attributeName>
                    <mode>yyyy</mode>
                </data>
            </content>
        </UITextField>
        <!--月-->
        <UITextField>
            <origin x="155" y="18" />
            <size width="8" height = "6"/>
            <content>
                <data>
                    <attributeName>inspectiondate</attributeName>
                    <mode>MM</mode>
                </data>
            </content>
        </UITextField>
        <!--日-->
        <UITextField>
            <origin x="168" y="18" />
            <size width="8" height = "6"/>
            <content>
                <data>
                    <attributeName>inspectiondate</attributeName>
                    <mode>dd</mode>
                </data>
            </content>
        </UITextField>
        <!--检查时间 开始-->
        <UITextField>
            <origin x="69" y="38" />
            <size width="74" height = "10"/>
            <content>
                <data>
                    <attributeName>timestart1</attributeName>
                    <mode>HH:mm</mode>
                </data>
            </content>
        </UITextField>
        <!--检查时间 结束-->
        <UITextField>
            <origin x="85" y="38" />
            <size width="74" height = "10"/>
            <content>
                <data>
                    <attributeName>timeend1</attributeName>
